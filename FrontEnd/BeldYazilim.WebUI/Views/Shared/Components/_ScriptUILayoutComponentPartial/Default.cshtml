<script>
    document.addEventListener('DOMContentLoaded', () => {
        ClassicEditor
            .create(document.querySelector('#editor'), {
                // Boş toolbar, düzenleme yeteneklerini devre dışı bırakır
                toolbar: []

            })
            .then(editor => {
                // readOnly modunu etkinleştir
                editor.enableReadOnlyMode('my-feature-id');

                // readOnly modunu konsola yazdır

            })
            .catch(error => {
                console.error(error);
            });
    });
</script>

